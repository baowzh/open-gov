<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.asiainfo.portal.modules.news.model.NewsData">
	<resultMap id="newsMap"
		type="com.asiainfo.portal.modules.news.model.NewsData">
		<result column="id" property="id" />
		<result column="content" property="content" />
	</resultMap>

	<insert id="insert" parameterType="com.asiainfo.portal.modules.news.model.NewsData">

		Insert into
		td_f_news_data
		(
		id,
		content
		)
		values (
		#{id,jdbcType=INTEGER},
		#{content,jdbcType=INTEGER}

		)

	</insert>
	<update id="update" parameterType="com.asiainfo.portal.modules.news.model.NewsData">
		updae td_f_news
		set id=#{id}
		<if test="content!=null and content!=''">
			,content=#{content}
		</if>

		where id=#{id}
	</update>
	<delete id="del" parameterType="java.lang.Integer">
		delete from td_f_news_data where id=#{id}
	</delete>


</mapper>