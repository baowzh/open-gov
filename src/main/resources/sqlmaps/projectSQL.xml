<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.asiainfo.portal.modules.project.model.Project">
    <resultMap id="projectMap" type="com.asiainfo.portal.modules.project.model.Project">
		<result column="id" property="id"  />
		<result column="name" property="name"  />
		<result column="depart_Id" property="departId"  />
		<result column="level" property="level"  />
		<result column="amount" property="amount"  />
		<result column="inputDate" property="inputDate"  />
		<result column="release_time" property="releaseTime"  />
		<result column="orgId" property="orgId" />
		<result column="townId" property="townId" />
		<result column="comm" property="comm"  />
		<result column="hamletId" property="hamletId"  />
		<result column="orgName" property="orgName"  />
		<result column="townName" property="townName"  />
        <result column="subcat" property="subcat"  />
        <result column="catName" property="catName"  />
        <result column="hamletName" property="hamletName"  />
        
        
	</resultMap>

	<insert id="insert" parameterType="com.asiainfo.portal.modules.project.model.Project">

		Insert into
		td_f_project
		(
		name,
		departId,
		level,
		amount,
		inputDate,
		releaseTime,
		orgId,
		townId,
		hamletId
		)
		values (
		#{name,jdbcType=VARCHAR},
		#{departId,jdbcType=VARCHAR},
		#{level,jdbcType=INTEGER}
		#{amount,jdbcType=DOUBLE}
		#{inputDate,jdbcType=DATE}
		#{releaseTime,jdbcType=DATE}
		#{orgId,jdbcType=VARCHAR}
		#{townId,jdbcType=VARCHAR}
		#{hamletId,jdbcType=VARCHAR}

		)

	</insert>
	<update id="update" parameterType="com.asiainfo.portal.modules.project.model.Project">
		updae td_f_project
		set id=#{id}
		<if test="catid!=null">
			,name=#{name}
		</if>
		<if test="departId!=null">
			,departId=#{departId}
		</if>
		<if test="level!=null">
			,level=#{level}
		</if>
		<if test="amount!=null">
			,amount=#{amount}
		</if>
		<if test="inputDate!=null">
			,inputDate=#{inputDate}
		</if>
		<if test="releaseTime!=null">
			,releaseTime=#{releaseTime}
		</if>
		<if test="orgId!=null">
			,orgId=#{orgId}
		</if>
		<if test="townId!=null">
			,townId=#{townId}
		</if>
		<if test="hamletId!=null">
			,hamletId=#{hamletId}
		</if>
		where id=#{id}
	</update>
	<delete id="del" parameterType="java.util.Map">
		delete from td_f_project
		where departId=#{departId}
		<if test="id!=null">
			id=#{id}
		</if>
	</delete>
	<select id="paging" parameterType="com.asiainfo.portal.form.ProjectPagingForm" resultMap="projectMap">
	  select * from td_f_project where depart_id=#{departId}
	</select>


</mapper>